/* Backup folder */
BACKUP ~weidu_external/gavin/backup~

/* Author */
SUPPORT ~https://www.gibberlings3.net/forums/forum/128-gavin/~ //berelinde@gmail.com

/* enable all error messages; nothing suppressed. comment this out for release version */
//MODDER

VERSION ~15~

/* launch the readme file immediately. */
README ~gavin/readme-gavin-%LANGUAGE%.html~ ~gavin/readme-gavin.html~

ALWAYS

  /* check for a ready SoD */

  ACTION_IF ((FILE_EXISTS ~dlc/sod-dlc.zip~) OR (FILE_EXISTS ~sod-dlc.zip~)) THEN BEGIN
    FAIL @4
  END
  
  ACTION_IF !VARIABLE_IS_SET cd_gavin_bg_always THEN BEGIN // check to make this happen only once per install 
      
    OUTER_SET cd_gavin_bg_always = 1
    
    INCLUDE ~gavin/lib/cd_portrait_copy.tph~

    /* These actions are processed for every component: reading of variables */

    ACTION_IF GAME_IS ~tutu tutu_totsc~ THEN BEGIN
      /* Tell the player it is using Tutu stuff */
      PRINT @0
      INCLUDE ~gavin\lib\g3_tutu_cpmvars.tpa~
    END ELSE
    ACTION_IF GAME_IS ~bgt~ THEN BEGIN
      /* Tell the player it is using BGT stuff */
      PRINT @1
      INCLUDE ~gavin\lib\g3_bgt_cpmvars.tpa~
     END ELSE 
     ACTION_IF GAME_IS ~bgee~ THEN BEGIN
      /* Tell the player it is using BG:EE stuff */
      PRINT @2000
      INCLUDE ~gavin\lib\g3_bgee_cpmvars.tpa~
     END ELSE BEGIN 
      /* Tell the player it is using eet */
      PRINT @104 
      INCLUDE ~EET/other/cpmvars/eet_cpmvars.tpa~
      INCLUDE ~EET/other/EET_functions.tph~
    END


/* Handling of correctly formatted tra-files!! ---------------------- */

  /*
   * Copies tra files into the autotra-folder (to leave the originals untouched)
   */
    COPY ~gavin/tra/english~  ~gavin/tra/autotra/%LANGUAGE%~ EVALUATE_BUFFER
    COPY ~gavin/tra/%LANGUAGE%~  ~gavin/tra/autotra/%LANGUAGE%~ EVALUATE_BUFFER


    ACTION_DEFINE_ARRAY fl#reload BEGIN gavin END

    LAF HANDLE_CHARSETS
      INT_VAR
        infer_charset = 1
      STR_VAR
        tra_path = EVAL ~gavin/Tra/autotra~
        noconvert_array = fl#noconvert
        reload_array = fl#reload
    END
    
  END // always var check  

END //ALWAYS


/* Language Settings */
AUTO_TRA ~gavin/tra/autotra/%s~

LANGUAGE ~English~ ~english~ ~gavin/tra/english/gavin.tra~
                             ~gavin/tra/english/setup.tra~
LANGUAGE ~French~  ~french~  ~gavin/tra/english/gavin.tra~
                             ~gavin/tra/english/setup.tra~
                             ~gavin/tra/french/gavin.tra~
                             ~gavin/tra/french/setup.tra~
LANGUAGE ~Deutsch (Teildeutsche Version, der Rest ist auf Englisch)~ ~german~ ~gavin/tra/english/gavin.tra~
                                                                              ~gavin/tra/english/setup.tra~
                                                                              ~gavin/tra/german/gavin.tra~
                                                                              ~gavin/tra/german/setup.tra~
 LANGUAGE ~Russian~ ~russian (by Austin & Arcanecoast Team)~ ~gavin/tra/english/gavin.tra~
                                                             ~gavin/tra/english/setup.tra~
                                                             ~gavin/tra/russian/gavin.tra~
